
    model{
    
    # Priors 
      lpsi ~ dnorm(0, 0.1)
      b1 ~   dnorm(0, 0.1)
      p ~   dunif(0, 1)
    
    # Ecological model, process model (true occurrence at site i) 
    for (i in 1:nsite) {                                           #loop sobre sitios 
          logit(psi[i]) <- lpsi + b1*is[i]
    
          z[i] ~ dbin(psi[i],1)

          tmp[i] <- p * z[i]
          y[i] ~ dbin(tmp[i], J[i])
     } #nsite
    
    }
    
  

